<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title | آروین تاو</title>
    <link rel="shortcut icon" href="favicon.png">
    <!-- plugins css -->
    <link rel="stylesheet" href="~/Content/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/bootstrap/css/bootstrap.min.css" />
    <link href="~/Content/UIkit/styles/uikit.min.css" rel="stylesheet" />
    <!--tagsinput-->
    <link rel="stylesheet" href="~/Content/bootstrap/css/bootstrap-tagsinput.css">
    <!-- font-icon-->
    <link rel="stylesheet" href="~/fonts/fontawesome/css/all.css">
    <link rel="stylesheet" href="~/fonts/fontawesome/css/fontawesome.css">
    <!-- custom css-->
    <link href="~/Areas/Admin/style.css" rel="stylesheet">
    <link href="~/Areas/Admin/style-rtl.css" rel="stylesheet">

    <link href="~/Content/Styles/Admin.min.css" rel="stylesheet" />
    @*<link href="~/Content/Styles/Controls.min.css" rel="stylesheet" />*@
    <link href="~/Tools/Chart/Chart.css" rel="stylesheet" />
</head>
<body>

    <div class="wrapper flex-div">
        <div class="layout flex-div">
            <!-- Side Nav START -->
            <div class="side-nav">
                <div class="side-nav-inner">
                    <div class="side-nav-logo">
                        <a href="#" class="uk-margin-auto row">
                            <img class="logo logo-darkt" src="~/img/Admin/logo/ArvinTavLogo.svg" />
                        </a>
                        <div class="mobile-toggle side-nav-toggle">
                            <a href="#">
                                <i class="fas fa-chevron-left"></i>
                            </a>
                        </div>
                    </div>
                    <ul class="side-nav-menu scrollable">
                        <li class="nav-item active">
                            <a href="/Admin/Dashboard">
                                <span class="icon-holder">
                                    <i class="fas fa-home"></i>
                                </span>
                                <span class="title">داشبورد</span>
                            </a>
                        </li>



                        <li class="nav-item dropdown">
                            <a class="dropdown-toggle" href="javascript:void(0);">
                                <span class="icon-holder">
                                    <i class="fas fa-th-large"></i>
                                </span>
                                <span class="title">خدمات</span>
                                <span class="arrow">
                                    <i class="fas fa-chevron-right"></i>
                                </span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="nav-item active">
                                    <a href="/Admin/ServiceCategory">
                                        <span class="icon-holder">
                                            <i class="fas fa-layer-group"></i>
                                        </span>
                                        <span class="title">دسته بندی خدمات</span>
                                    </a>
                                </li>
                                <li class="nav-item active">
                                    <a href="/Admin/Hosting">
                                        <span class="icon-holder">
                                            <i class="fas fa-server"></i>
                                        </span>
                                        <span class="title">میزبانی</span>
                                    </a>
                                </li>
                                <li class="nav-item active">
                                    <a href="/Admin/Domain">
                                        <span class="icon-holder">
                                            <i class="fas fa-globe-americas"></i>
                                        </span>
                                        <span class="title">دامنه</span>
                                    </a>
                                </li>
                                <li class="nav-item active">
                                    <a href="/Admin/Package">
                                        <span class="icon-holder">
                                            <i class="fas fa-box"></i>
                                        </span>
                                        <span class="title">بسته های نرم افزاری</span>
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <li class="nav-item active">
                            <a href="/Admin/Order">
                                <span class="icon-holder">
                                    <i class="fas fa-shopping-cart"></i>
                                </span>
                                <span class="title">سفارشات</span>
                            </a>
                        </li>
                        <li class="nav-item active">
                            <a href="/Admin/Ticket">
                                <span class="icon-holder">
                                    <i class="fas fa-headset"></i>
                                </span>
                                <span class="title">مرکز پاسخگویی</span>
                            </a>
                        </li>
                        <li class="nav-item active">
                            <a href="/Admin/Project">
                                <span class="icon-holder">
                                    <i class="fas fa-project-diagram"></i>
                                </span>
                                <span class="title">پروژه ها</span>
                            </a>
                        </li>
                        <li class="nav-item active">
                            <a href="/Admin/User">
                                <span class="icon-holder">
                                    <i class="fas fa-users"></i>
                                </span>
                                <span class="title">کاربران</span>
                            </a>
                        </li>
                        <li class="nav-item active">
                            <a href="/Admin/Festival">
                                <span class="icon-holder">
                                    <i class="fas fa-gift"></i>
                                </span>
                                <span class="title">جشنواره</span>
                            </a>
                        </li>
                             <li class="nav-item active">
                                 <a href="/Admin/Marketer">
                                     <span class="icon-holder">
                                         <i class="fas fa-user-tie"></i>
                                     </span>
                                     <span class="title">نمایندگی فروش</span>
                                 </a>
                             </li>
                        <li class="nav-item active">
                            <a href="/Admin/Management">
                                <span class="icon-holder">
                                    <i class="fas fa-user-shield"></i>
                                </span>
                                <span class="title">مدیریت</span>
                            </a>
                        </li>
                        <li class="nav-item active">
                            <a href="/Admin/HomeSetting">
                                <span class="icon-holder">
                                    <i class="fas fa-cog"></i>
                                </span>
                                <span class="title">تنظیمات خانه</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <!-- Side Nav END -->
            <!-- Page Container START -->
            <div class="page-container flex-div">
                <!-- Header START -->
                <div class="header navbar">
                    <div class="header-container">
                        <ul class="nav-right float-right mr-3">
                            <li>
                                <a class="side-nav-toggle" href="javascript:void(0);">
                                    <i class="fas fa-align-right"></i>
                                </a>
                            </li>
                        </ul>
                        <ul class="nav-left float-left">
                            @Html.Action("P_UserBox","Dashboard", new { area = "Admin" })
                            @Html.Action("notification","Dashboard", new { area = "Admin" })
                        </ul>

                    </div>
                </div>
                <div class="sub-header">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb rounded-0 m-0 bg-white">
                            <li class="breadcrumb-item p-0"><a href="/admin/Home">داشبورد</a></li>
                            <li class="breadcrumb-item p-0 active" aria-current="page">@ViewBag.Title</li>
                        </ol>
                    </nav>
                </div>
                <!-- Header END -->
                <!-- Content Wrapper START -->
                <div class="main-content flex">
                    <div class="container-fluid flex-div">
                        @RenderBody()
                    </div>
                </div>
                <!-- Content Wrapper END -->
                <!-- Footer START -->
                <footer class="content-footer bg-white">
                    <div class="footer border-0">
                        <div class="copyright">
                            <span><b class="text-dark">آروین تاو</b> 2020</span>
                        </div>
                    </div>
                </footer>
                <!-- Footer END -->
            </div>
            <!-- Page Container END -->
        </div>
    </div>
</body>
</html>

<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/popper.min.js"></script>
<script src="~/Scripts/docs.min.js"></script>
<script src="~/Content/bootstrap/js/bootstrap.js"></script>
<script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
<script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
<script src="~/Areas/Admin/script.js"></script>
<script src="~/Content/bootstrap/js/bootstrap-tagsinput.js"></script>
<script src="~/Tools/ckeditor/ckeditor.js"></script>
<script src="~/Tools/ckeditor/adapters/jquery.js"></script>
<script src="~/Scripts/UIkit/uikit.min.js"></script>
<script src="~/Scripts/UIkit/uikit-icons.min.js"></script>
<script src="~/Tools/Chart/Chart.js"></script>

@RenderSection("Scripts", false)
