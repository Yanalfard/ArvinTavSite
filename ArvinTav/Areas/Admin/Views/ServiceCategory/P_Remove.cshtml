@model DataLayer.ServiceCategory
<div class="modal-header">
    <h6 class="modal-title" id="modalCenterTitle">حذف دسته بندی @Model.Title</h6>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="space"></div>
<h6 class="center">آیا میخواهید دسته بندی @Model.Title  حذف شود؟</h6>
<div class="space"></div>
<div class="modal-footer justify-content-end">
    <img src="~/img/gif/load.gif" class="loding justify-content-center d-none" />
    <button onclick="ConfirmDelete(@Model.ID)" type="button" class="btn btn-sm btn-success">
        بله
    </button>
    <button type="button" class="btn btn-sm btn-danger" data-dismiss="modal" aria-label="Close">خیر</button>

</div>

<script src="~/Areas/Admin/CommandJs/ServiceCategory.js"></script>

