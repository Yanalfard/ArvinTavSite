@model DataLayer.LandingViewModel
@{
    ViewBag.Title = "شرکت ایده پرداز آروین تاو";
    ViewBag.Description = "مجموعه طراحی سایت با بیش 15 سال سابقه کار در زمینه طراحی فروشگاه اینترنتی و سایت اختصاصی";
    ViewBag.Keywords = "طراحی سایت,طراحی فروشگاه اینترنتی,طراحی سایت اختصاصی";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<div class="IndexPage">
    <div class="headSlider uk-position-relative uk-visible-toggle uk-light" tabindex="-1" uk-slideshow="autoplay:true" dir="rtl">
        <ul class="uk-slideshow-items">
            @foreach (var item in Model.AllSlider)
            {
                <li class="overflow-visible">
                    <img src="~/Document/img/Slider/@item.Image" alt="web-design" width="550" height="550" />
                    <div class="coverContent uk-position-left uk-position-small uk-text-center center">
                        <h3 uk-slideshow-parallax="x: 100,-100"></h3>
                        <p class="uk-text-center" uk-slideshow-parallax="x: 200,-200" dir="rtl">
                            @item.Title
                        </p>
                        <a href="@item.Link" class="ServiceButton">مشاهده</a>
                    </div>
                </li>
            }
        </ul>

        <a class="uk-slidenav-container uk-position-center-left uk-position-small uk-hidden-hover" href="#" uk-slidenav-previous uk-slideshow-item="previous"></a>
        <a class="uk-slidenav-container uk-position-center-right uk-position-small uk-hidden-hover" href="#" uk-slidenav-next uk-slideshow-item="next"></a>

    </div>
    <div class="endOfCover"></div>
    <!--end region -->
    <!--sponsors region -->
    <h3 class="TitrOfHoldingProjects center uk-position-absolute ">پروژه های هلدینگ آروین تاو</h3>
    <div class="sponsor uk-position-relative uk-visible-toggle uk-light" tabindex="-1" uk-slider="autoplay: true ; autoplay-interval:4000 ">
        <ul class="uk-slider-items uk-child-width-1-2 uk-child-width-1-4@m uk-grid  center overflow-visible">

            @foreach (var item in Model.partners)
            {
                <li>
                    <a><img src="~/Document/img/Partner/@item.Logo" width="80" height="80" alt="logo" /></a>
                </li>
            }

        </ul>
        <a class="uk-s-slider-container uk-position-center-left uk-position-small uk-hidden-hover" href="#" uk-slidenav-previous uk-slider-item="previous"></a>
        <a class="uk-s-slider-container uk-position-center-right uk-position-small uk-hidden-hover" href="#" uk-slidenav-next uk-slider-item="next"></a>
    </div>
    <!--end region -->
    <hr />
    <!--services region-->
    <div class="my-column my-clipped" dir="rtl">
        <content class="Services my-row uk-padding">
            <div class="my-column my-clipped" uk-scrollspy="cls: uk-animation-slide-right; repeat:false" dir="rtl">
                <h2>طراحی سایت</h2>
                <img class="center MobileSize" uk-scrollspy="cls: uk-animation-slide-left; repeat: false " src="~/img/Home/svg/Waterfall/Website.svg" width="180" height="180" alt="picture" />
                <p> خدمات طراحی و توسعه وب سایت های کاربردی و عملیاتی و طراحی پورتال، برنامه نویسی و توسعه انواع نرم افزارهای کاربردی تحت وب، با توجه به نیازها، نوع فعالیت و متناسب با بودجه در نظر گرفته شده توسط مشتری صورت می پذیرد. در بعضی از موارد يک وب سايت زيبا به تنهايي کارساز نيست و برای موفقيت در تجارت بايد بتوانيد ارتباط قوي تری با بازديدکنندگان خود داشته باشيد و یا اینکه عملیاتی را نیاز دارید که میتواند به ارائه هرچه بهتر خدمات شما کمک کند. </p>
                <a class="ServiceButton" href="/MainPackage/10/Asp">بیشتر</a>
            </div>
            <img class="DesktopSize" uk-scrollspy="cls: uk-animation-slide-left; repeat: false " src="~/img/Home/svg/Waterfall/Website.svg" width="280" height="280" alt="picture" />
        </content>
        <content class="Services my-row uk-padding">
            <img class="DesktopSize" uk-scrollspy="cls: uk-animation-slide-right; repeat:false" src="~/img/Home/svg/Waterfall/Mobile.svg" width="280" height="280" alt="picture" />
            <div class="my-column my-clipped" uk-scrollspy="cls: uk-animation-slide-left; repeat: false " dir="rtl">
                <h2>طراحی اپلیکیشن موبایل</h2>
                <img class="center MobileSize" uk-scrollspy="cls: uk-animation-slide-right; repeat:false" src="~/img/Home/svg/Waterfall/Mobile.svg" width="180" height="180" alt="picture" />
                <p> خدمات طراحی و توسعه وب سایت های کاربردی و عملیاتی و طراحی پورتال، برنامه نویسی و توسعه انواع نرم افزارهای کاربردی تحت وب، با توجه به نیازها، نوع فعالیت و متناسب با بودجه در نظر گرفته شده توسط مشتری صورت می پذیرد. در بعضی از موارد يک وب سايت زيبا به تنهايي کارساز نيست و برای موفقيت در تجارت بايد بتوانيد ارتباط قوي تری با بازديدکنندگان خود داشته باشيد و یا اینکه عملیاتی را نیاز دارید که میتواند به ارائه هرچه بهتر خدمات شما کمک کند. </p>
                <a class="ServiceButton" href="/MainPackage/13/اندروید-و-IOS">بیشتر</a>
            </div>
        </content>
        <content class="Services my-row uk-padding">
            <div class="my-column my-clipped" uk-scrollspy="cls: uk-animation-slide-right; repeat:false" dir="rtl">
                <h2>آموزش آنلاین</h2>
                <img class="center MobileSize" uk-scrollspy="cls: uk-animation-slide-left; repeat: false " src="~/img/Home/svg/Waterfall/Learning.svg" width="180" height="180" alt="picture" />
                <p> خدمات طراحی و توسعه وب سایت های کاربردی و عملیاتی و طراحی پورتال، برنامه نویسی و توسعه انواع نرم افزارهای کاربردی تحت وب، با توجه به نیازها، نوع فعالیت و متناسب با بودجه در نظر گرفته شده توسط مشتری صورت می پذیرد. در بعضی از موارد يک وب سايت زيبا به تنهايي کارساز نيست و برای موفقيت در تجارت بايد بتوانيد ارتباط قوي تری با بازديدکنندگان خود داشته باشيد و یا اینکه عملیاتی را نیاز دارید که میتواند به ارائه هرچه بهتر خدمات شما کمک کند. </p>
                <a class="ServiceButton" href="/MainPackage/14/مدرسه-آنلاین">بیشتر</a>
            </div>
            <img class="DesktopSize" uk-scrollspy="cls: uk-animation-slide-left; repeat: false " src="~/img/Home/svg/Waterfall/Learning.svg" width="280" height="280" alt="picture" />
        </content>
    </div>

    <!--end region -->

    <div class="space"></div>
    <div class="space-mobile"></div>
    <!--projects region-->
    <h3 class="TitrOfProjects center uk-position-absolute ">مشتریان ما</h3>

    <div class="projectsSlider GreenBackgroundPro my-row center ">

        <div class="uk-width-3-4 GreenBackground uk-margin-small-right center">
            <div class="projects uk-position-relative uk-visible-toggle uk-light" tabindex="-1" uk-slider="autoplay: true ; autoplay-interval:4000 ; pause-on-hover:false">
                <ul class="uk-slider-items uk-child-width-1-2@s uk-child-width-1-3@m uk-child-width-1-4@l uk-child-width-1-4@xl uk-grid  center overflow-visible">

                    @foreach (var item in Model.projects)
                    {
                        <li>
                            <a href="@item.Link"><img src="~/Document/img/Project/@item.Image" width="60" height="60" alt="logo" /></a>
                        </li>
                    }

                </ul>
                <a class="uk-p-slidenav-container uk-position-center-left uk-hidden-hover" href="#" uk-slidenav-previous uk-slider-item="previous"></a>
                <a class="uk-p-slidenav-container uk-position-center-right uk-hidden-hover" href="#" uk-slidenav-next uk-slider-item="next"></a>
            </div>
        </div>
        <div class="vLine"></div>
        <div class="uk-width-1-4 GreenBackground center uk-margin-small-left">
            <img class="cenetr uk-margin-auto-top uk-margin-auto-bottom" src="~/img/Home/namde etemad.png" width="200" height="200" />
        </div>
    </div>
    <!--end region -->
    <!--why arvintov region-->
    <div class="whyArvin my-column my-clipped uk-margin-top" dir="rtl">
        <content class="Services my-row uk-padding">
            <div class="my-column my-clipped" uk-scrollspy="cls: uk-animation-slide-right; repeat:false" dir="rtl">
                <h2>چرا آروین تاو</h2>
                <img class=" MobileSize center" uk-scrollspy="cls: uk-animation-slide-left; repeat:false" src="~/img/Home/why.svg" width="250" height="250" alt="picture" />
                <div class="whyComponent" dir="rtl">
                    <ul>
                        <li><img src="~/img/Home/svg/Why/Success.svg" width="24" height="24" alt="picture" /><labal dir="rtl">توجه به موفقیت مشتریان</labal></li>
                        <li><img src="~/img/Home/svg/Why/Analize.svg" width="24" height="24" alt="picture" /><labal>مشاوره و آنالیز دقیق کسب و کار قبل از راه اندازی تجارت الکترونیک شما</labal>
                        <li><img src="~/img/Home/svg/Why/Experience.svg" width="24" height="24" alt="picture" /><labal>تجربه قدرتمند و به روز</labal></li>
                        <li><img src="~/img/Home/svg/Why/Foundation.svg" width="24" height="24" alt="picture" /><labal>تخصصی ترین زیرساخت ها برای الکترونیکی سازی فعالیت های شما</labal></li>
                        <li><img src="~/img/Home/svg/Why/Fill.svg" width="24" height="24" alt="picture" /><labal>سبد کامل محصولات و خدمات اختصاصی شده برای کسب و کار شما</labal></li>
                        <li><img src="~/img/Home/svg/Why/Content.svg" width="24" height="24" alt="picture" /><labal>تولید محتوا ی حرفه ای</labal></li>
                        <li><img src="~/img/Home/svg/Why/Quality.svg" width="24" height="24" alt="picture" /><labal>توجه به کیفیت</labal></li>
                        <li><img src="~/img/Home/svg/Why/Team.svg" width="24" height="24" alt="picture" /><content>تیم بزرگی از متخصصان حوزه تجارت الکترونیک و برنامه نویسی متخصص</content></li>
                    </ul>
                </div>
            </div>
            <img class=" DesktopSize uk-margin-auto-top uk-margin-auto-bottom" uk-scrollspy="cls: uk-animation-slide-left; repeat:false" src="~/img/Home/why.svg" width="280" height="280" alt="picture" />
        </content>

    </div>


    <!--end region>-->
    <!--packages-->
    <div class="my-clipped my-column ">
        <div class="Packages center">

            @if (Model.packageServices.Count() > 3)
            {
                <article class="packageOffer column uk-padding center uk-margin-remove">
                    <content>
                        <i class="far fa-gem fa-2x"></i>
                        <h3>@Model.packageServices.ToList()[0].Title</h3>
                        <div class="space-half"></div>
                        <p>@Html.Raw(Model.packageServices.ToList()[0].Description)</p>
                        <div class="space"></div>
                        <div class="space"></div>
                        <div class="space"></div>
                        <a class="ServiceButton" href="/Product/@Model.packageServices.ToList()[0].ID/@Model.packageServices.ToList()[0].Title.Replace(" ","-")">سفارش</a>
                    </content>
                </article>
                <article class="topPackage column uk-padding center uk-margin-remove ">
                    <content>
                        <i class="far fa-gem fa-2x"></i>
                        <h3>@Model.packageServices.ToList()[1].Title</h3>
                        <div class="space-half"></div>
                        <p>@Html.Raw(Model.packageServices.ToList()[1].Description)</p>
                        <div class="space"></div>
                        <div class="space"></div>
                        <div class="space"></div>
                        <a class="ServiceButton" href="/Product/@Model.packageServices.ToList()[1].ID/@Model.packageServices.ToList()[1].Title.Replace(" ","-")">سفارش</a>

                    </content>
                </article>
                <article class="packageOffer column uk-padding center uk-margin-remove ">
                    <content>
                        <i class="far fa-gem fa-2x"></i>
                        <h3>@Model.packageServices.ToList()[2].Title</h3>
                        <div class="space-half"></div>
                        <p>@Html.Raw(Model.packageServices.ToList()[2].Description)</p>
                        <div class="space"></div>
                        <div class="space"></div>
                        <div class="space"></div>
                        <a class="ServiceButton" href="/Product/@Model.packageServices.ToList()[2].ID/@Model.packageServices.ToList()[2].Title.Replace(" ","-")">سفارش</a>

                    </content>
                </article>
            }

        </div>

    </div>
    <!--end region-->
    <!--about region-->
    <div class="space"></div>
    <div class="AboutArvin">
        <div class="my-column my-clipped" dir="rtl">
            <content class="Services my-row uk-padding">
                <div class="my-column my-clipped" dir="rtl">
                    <h2>ما کیستیم</h2>
                    <img class=" MobileSize center" src="~/img/Home/svg/WorkFlow.svg" width="180" height="180" alt="picture" />
                    <p>
                        شرکت ایده پرداز آروین تاو با پشتوانه ی تخصص اعضا و تیم های اجرایی متخصص خود در حوزه فناوری اطلاعات، تجارت الکترونیکی، طراحی و توسعه وب سایت های اختصاصی و عملیاتی، برنامه نویسی و توسعه انواع نرم افزار های تحت وب و تحت ویندوز و تحت موبایل( اندروید و IOS) با بروز ترین تکنولوژی های روز دنیا و با هدف  الکترونیکی کردن کسب و کارها و یکپارچه سازی تمامی این فعالیت ها با تشکیل تیمهای قدرتمند و متخصص سعی بر این دارد زیر ساختهایی را بوجود آورد که مردم و کسب و کارها از مزیت های تکنولوژی و تجارت الکترونیکی به ساده ترین شکل ممکن بهره مند شوند و در هزینه های خود بطور چشمگیری صرفه جویی کنند شرکت ایده پرداز آروین تاو برای هر یک از خواسته های شما تیم های قدرتمندی متشکل از برنامه نویسان حرفه ای تدارک دیده است تا به تمامی اهداف شما جامه عمل بپوشاند و شما را در رسیدن به اهدافتان یاری نماید و همچنین با از بین بردن مرزهای جغرافیای دایره مشتریانتان را گسترش دهد درحالی که بطور قابل توجهی تا 80% از هزینه های اضافی شما را کاهش میدهد
                    </p>
                </div>
                <img class=" DesktopSize uk-margin-auto-top uk-margin-auto-bottom" src="~/img/Home/svg/WorkFlow.svg" width="280" height="280" alt="picture" />
            </content>

        </div>

        <!--end region-->

    </div>
    <!--end region-->
</div>
<script src="~/Scripts/UIkit/uikit.min.js"></script>

